<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.a4ia1.photosmanager.Activities.ChooseCollage">
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Simple 1"
        android:id="@+id/collage_choose_simple_1"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Simple 2"
        android:id="@+id/collage_choose_simple_2"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Complicated 1"
        android:id="@+id/collage_choose_complicated_1"/>

</LinearLayout>
